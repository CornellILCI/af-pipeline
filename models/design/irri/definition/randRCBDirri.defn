      "nRowPerRep": {
        "variable_label": "No. of rows per rep",
        "description": "Number of rows per replicate",
        "visible": "T",
        "minimum": "1",
        "maximum": null,
        "unit": null,
        "values": null,
        "default": 1,
        "disabled": "F",
        "required": "T",
        "data_type": "integer",
        "order_number": 5,
        "variable_abbrev": "NO_OF_ROWS_PER_BLOCK",
        "is_layout_variable": "T",
        "rule": [
          {
            "type": "allowed-value",
            "expression": "factor(totalEntries)"
          },
          {
            "type": "allowed-value",
            "expression": "max=totalEntries"
          }
        ]
      },
      "nFieldRow": {
        "variable_label": "Rows",
        "description": "Total number of rows",
        "visible": "T",
        "minimum": "1",
        "maximum": null,
        "unit": null,
        "values": null,
        "default": null,
        "disabled": "F",
        "required": "T",
        "data_type": "integer",
        "order_number": 6,
        "variable_abbrev": "ROW",
        "is_layout_variable": "T",
        "rule": [
          {
            "type": "allowed-value",
            "expression": "min = nRowPerRep"
          },	
          {
            "type": "allowed-value",
            "expression": "divisible by nRowPerRep"
          },
          {
            "type": "allowed-value",
            "expression": "quotient nFieldRow/nRowPerRep should be a factor of nRep"
          },
          {
            "type": "allowed-value",
            "expression": "max=nRowPerRep*nRep"
          }
        ]
      },
